{
  "name": "jest-environment-puppeteer",
  "description": "Run your tests using Jest & Puppeteer.",
  "version": "0.1.0",
  "main": "index.js",
  "repository": "git@github.com:smooth-code/jest-environment-puppeteer.git",
  "author": "Greg Berg√© <berge.greg@gmail.com>",
  "license": "MIT",
  "keywords": [
    "jest",
    "jest-environment",
    "puppeteer",
    "jest-puppeteer",
    "chromeless",
    "chrome-headless"
  ],
  "scripts": {
    "build": "rm -rf lib/ && babel src -d lib",
    "ci": "yarn build && yarn lint && yarn test",
    "dev": "babel src -d lib --watch",
    "format": "prettier --write \"src/**/*.js\" \"*.md\" \"*.js\"",
    "lint": "eslint .",
    "release": "standard-version && conventional-github-releaser -p angular",
    "test": "jest",
    "prepublishOnly": "yarn run build"
  },
  "peerDependencies": {
    "jest-environment-node": "^22.0.0",
    "puppeteer": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-preset-env": "^1.6.1",
    "conventional-github-releaser": "^2.0.0",
    "eslint": "^4.18.2",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.9.0",
    "jest": "^22.4.2",
    "jest-environment-node": "^22.4.1",
    "puppeteer": "^1.1.1",
    "standard-version": "^4.3.0"
  },
  "dependencies": {
    "cwd": "^0.10.0",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.6.2"
  }
}
